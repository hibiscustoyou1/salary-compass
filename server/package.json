{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "build": "pnpm prisma:dep && pnpm esbuild && pnpm cp-dep",
    "esbuild": "npx esbuild src/index.ts --bundle --platform=node --target=node22 --format=cjs --outfile=./dist/index.js",
    "cp-dep": "cp -f prisma/schema.prisma ./dist && cp -f ../node_modules/.pnpm/@prisma+client@5.22.0_prisma@5.22.0/node_modules/.prisma/client/libquery_engine-*.so.node ./dist",
    "prisma:migrate": "npx dotenv -e ../.env -- npx prisma migrate dev",
    "prisma:dep": "npx dotenv -e ../.env -- npx prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/cors": "^2.8.19",
    "dotenv-cli": "^11.0.0",
    "esbuild": "^0.27.0",
    "nodemon": "^3.1.11",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}